---
import { type Job } from "@/data/job.ts";
import Education from "./Education.astro";
import Divider from "@/components/Divider.astro";

const jobs: Job[] = [
  {
    alt: "Google logo",
    company: "Google",
    description: "",
    href: "google.com",
    img: "/img/Google.svg",
    title: "Brand Designer",
    timestamp: "2025 - Present",
  },
  {
    alt: "Facebook logo",
    company: "Facebook",
    description: "",
    href: "google.com",
    img: "/img/Facebook.svg",
    title: "Fullstack Developer",
    timestamp: "2023 - 2025",
  },
  {
    alt: "Twitter Logo",
    company: "Twitter",
    description: "",
    href: "google.com",
    img: "/img/Twitter.svg",
    title: "CEO",
    timestamp: "2020 - 2023",
  },
];
---

{
  jobs.map((job, index) => (
    <>
      {index > 0 && <Divider />}
      <Education job={job} />
    </>
  ))
}
